package model;

/**
 * @author DanielMontezuma
 * */
public class System{

 //Associations
 /**
 * type is a variable stored in the Wetland class that gives us the the type(urban,rural) of the wetland
 * */
 private Wetland type;
 /**
 * protectedArea is a variable stored in the wetland class that show if the wetland is a protected area or not
 * */
 private Wetland protectedArea;
 /**
 * migratorySpecies is a variable stored in the species class that show if the specie if migratory or not
 * */
 private Species migratorySpecies;
 /**
 * specie is a variable stored in the Species class that gives us the species of the wetland
 * */
 private Species specie;
 /**
 * type is a variable stored in the Event class that gives us the type of event of the wetland
 * */
 private Event eType;
 
 //Set
 
 /**
	* This method set the type of the wetland
	* @param Wetland type
	*/
 public void setType(Wetland type){
	 
	 this.type=type
 }
 /**
	* This method set if the wetland is a protected area
	* @param Wetland protectedArea
	*/
 public void setProtectedArea(Wetland protectedArea){
	 
	 this.protectedArea=protectedArea
 }
 /**
	* This method if the specie is a migratory specie
	* @param Species migratorySpecies
	*/
 public void setMigratorySpecies(Species migratorySpecies){
	 
	 this.migratorySpecies=migratorySpecies
 }
 /**
	* This method set the species for the Species class
	* @param Species species
	*/
 public void setSpecie(Species specie){
	 
	 this.specie=specie
 }
 /**
	* This method set the type of the event
	* @param Event eType
	*/
 public void setEType(Event eType){
	 
	 this.eType=eType
 }
 
 //Get
 
 /**
	* This method get the variables for the system
	* @return type show the type of the wetland, protected show the wetland is a protected Area, specie show the specie of the wetland, eType show the type of the event
	*/
 public String toString(){
	  
	  return type + "\n" + protectedArea + "\n" + migratorySpecies + "\n" + specie + "\n" + eType;
  }
 


}